{
  "name": "@wisemen/oauth2-vue-client",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.12.1",
  "description": "_description_",
  "author": "Wouter Laermans <wouter.laermans@wisemen.digital>, Jeroen Van Caekenberghe <jeroen.vancaekenberghe@wisemen.digital>",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "clean": "pnpm lint && pnpm typecheck && pnpm test",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "axios": "1.7.2"
  },
  "devDependencies": {
    "@antfu/ni": "0.21.12",
    "@antfu/utils": "0.7.8",
    "@types/node": "20.14.8",
    "@wisemen/eslint-config-vue": "0.1.4",
    "bumpp": "9.4.1",
    "eslint": "9.5.0",
    "esno": "4.7.0",
    "lint-staged": "15.2.7",
    "pnpm": "9.4.0",
    "rimraf": "5.0.7",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.5.2",
    "unbuild": "2.0.0",
    "vite": "5.3.1",
    "vitest": "1.6.0",
    "vitest-localstorage-mock": "^0.1.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
