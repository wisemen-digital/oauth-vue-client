{
  "name": "@wisemen/oauth2-vue-client",
  "type": "module",
  "version": "1.1.2",
  "packageManager": "pnpm@9.5.0",
  "description": "_description_",
  "author": "Wouter Laermans <wouter.laermans@wisemen.digital>, Jeroen Van Caekenberghe <jeroen.vancaekenberghe@wisemen.digital>",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "precommit": "pnpm lint && pnpm typecheck && pnpm test",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "axios": "1.7.7",
    "pkce-challenge": "4.1.0"
  },
  "devDependencies": {
    "@antfu/ni": "0.23.0",
    "@antfu/utils": "0.7.10",
    "@types/node": "22.7.8",
    "@wisemen/eslint-config-vue": "0.2.0",
    "axios-mock-adapter": "2.1.0",
    "bumpp": "9.7.1",
    "eslint": "9.13.0",
    "esno": "4.8.0",
    "lint-staged": "15.2.10",
    "pnpm": "9.12.2",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.6.3",
    "unbuild": "2.0.0",
    "vite": "5.4.9",
    "vitest": "2.1.3",
    "vitest-localstorage-mock": "0.1.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
